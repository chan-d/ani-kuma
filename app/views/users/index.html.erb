<h1>Users#index</h1>
<!-- flash messages -->
  <% flash.each do |name, msg| %>
    <% if name == "error" %>
      <p class="alert-danger">
    <% else %>
      <p class="alert-success">
    <% end %>
      <small> <%= name.capitalize %>: <%= msg %> </small>
    </p>
  <% end %>
<p>signed in as: <%= current_user.user_name %> </p>
<p>full name: <%= current_user.full_name %> </p>
<p>email: <%= current_user.email %> </p>
<p>created at: <%= current_user.created_at %> </p>
<p>about me: <%= current_user.about_me %> </p>